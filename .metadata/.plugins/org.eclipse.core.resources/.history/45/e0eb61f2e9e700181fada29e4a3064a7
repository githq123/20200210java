
public class Student {

	String no,name,sex,dept;
	int age;
	Student(){};
	Student(String no,String name,String sex,int age,String dept){
		this.no=no;
		this.name=name;
		this.sex=sex;
		this.age=age;
		this.dept=dept;
	}
	public String getNo() {
		return no;
	}
	public void setNo(String no) {
		this.no = no;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getSex() {
		return sex;
	}
	public void setSex(String sex) {
		this.sex = sex;
	}
	public String getDept() {
		return dept;
	}
	public void setDept(String dept) {
		this.dept = dept;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String toString() {
		String result = "no: "+no+", name: "+name+", sex: "+sex+", age: "+age+", dept: "+dept;
		return result;
	}
}
